{
  "name": "@innomotics/ix",
  "version": "0.0.1-alpha-24",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/innomotix-ix/innomotix-ix.esm.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "copyreadmes": "npx cpx \"./src/components/**/*.md\" \"../webdocs/docs/components\"",
    "createcolorsreadme": "npx ts-node colorreadmegenerator.ts && npx cpx \"./styles/readme.md\" \"../webdocs/docs/colors\"",
    "build": "stencil build && npm run copyreadmes && npm run createcolorsreadme",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "stencil": "stencil"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@innomotics/ix-fonts": "^0.0.1-alpha-24",
    "@innomotics/ix-icons": "^0.0.1-alpha-24",
    "@stencil/core": "^4.12.5",
    "@stencil/sass": "^3.0.10",
    "animejs": "^3.2.2"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/angular-output-target": "^0.8.4",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/vue-output-target": "^0.8.7",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "autoprefixer": "^10.4.17",
    "cpx2": "^7.0.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "peerDependencies": {
    "@innomotics/ix-fonts": "^0.0.1-alpha-24",
    "@innomotics/ix-icons": "^0.0.1-alpha-24"
  },
  "license": "MIT"
}
